      </div>
      <!-- Grid row -->
    </div>
  </section>
  <!-- Section: Links  -->

  <!-- Copyright -->
  <div class="text-center p-4" style="background-color: rgba(0, 0, 0, 0.025);">
    © 2022 Copyright:
    <a class="text-reset fw-bold" href="https://poeltl.dunk.town/">Hugo Boulhaut</a>
  </div>
  <!-- Copyright -->
</footer>
<!-- Footer -->
